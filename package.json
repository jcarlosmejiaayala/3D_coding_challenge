{
  "name": "3D_coding_challenge",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:jcarlosmejiaayala/3D_coding_challenge.git",
  "author": "jcarlosmejiaayala <jcarlosmejiaayala@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack",
    "dev": "webpack-dev-server --progress",
    "dev:docker": "yarn && yarn dev",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@wildpeaks/three-webpack-plugin": "^2.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "canvas": "^1.6.11",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^7.2.2",
    "mtl-loader": "^0.2.0",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "three": "^0.95.0"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix",
      "git add --force"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/assetsTransformer.js"
    },
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not ie 11",
              "not op_mini all"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
